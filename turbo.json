{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["test", "^build"], 
      "outputs": ["apps/web/.next"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
